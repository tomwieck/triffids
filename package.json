{
    "name": "triffids",
    "version": "0.1.1",
    "description": "Vue.js based app to explore parks and open spaces in Bristol, UK",
    "homepage": "https://triffids.app",
    "license": "GPL-3.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "npm install; vue-cli-service build",
        "lint": "vue-cli-service lint",
        "nuke": "rm -rf node_modules; rm -r dist/*"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "leaflet": "^1.5.1",
        "leaflet.locatecontrol": "^0.67.0",
        "node-sass": "^4.13.1",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.10",
        "vue-browser-geolocation": "^1.8.0",
        "vue-router": "^3.0.7",
        "vue-spinners": "^1.0.2",
        "vuejs-logger": "^1.5.3",
        "wikijs": "^5.5.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.9.2",
        "@vue/cli-plugin-eslint": "^3.9.2",
        "@vue/cli-service": "^3.9.2",
        "babel-eslint": "^10.0.2",
        "eslint": "^6.0.1",
        "eslint-plugin-vue": "^5.2.3",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
